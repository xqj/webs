@model Webs.Model.GridPage<Webs.Model.Info>
@{
    ViewBag.Title = "List";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>hsieh chingjing的技术阵地</title>
    <meta name="Keywords" content="互联网技术 无人化 智能硬件 大数据" />
    <meta name="Description" content="hsieh chingjing个人技术阵地：基于C# net mono的分布式系统实现与运维技术、基于hadoop与storm的智能分析技术、基于互联网技术的无人化企业研究" />
</head>
<body>
    <div class="logo"><h1> hsieh chingjing的技术阵地</h1><h5>技术理性派</h5></div>
    <h4>@ViewData["channelsData"]</h4>
    <div class="list">
        @{
            foreach (var k in Model.List)
            {
                <div>
                    <a href='@Url.Action("Detail")?id=@k.InfoId' target="_blank">@k.InfoTitle</a><br />
                   @Webs.Common.StringHelper.GetString(k.InfoContent, 50);
                </div>
            }
        }
    </div>
    <link href="http://r.xieqj.net/css/b/i.css?v=5" rel="stylesheet" />

</body>
</html>